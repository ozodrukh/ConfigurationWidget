{
  "Application Information": {
    "type": "BuildConfigInformation",
    "object": "codetail.green.sample.BuildConfig"
  },

  "Device Information": {
    "type": "DeviceInformationModule"
  },

  "Example Module":[
    {
      "key": "productFlavor",
      "type": "Spinner",
      "value": 1,
      "entities": {
        "#0": "ProductionMode",
        "#1": "DeveloperMode"
      }
    },
    {
      "key": "endpoint",
      "type": "EditText",
      "hint": "Endpoint url"
    }
  ],

  "Network Information":{
    "type": "NetworkInformation",
    "show_flag": "only_active_network",

    "comment": "(1000ms = 1s) * 10 = 10s"
  },

  "Traffic Stats":{
    "type": "NetworkInformation",
    "update_delay": 5000,
    "show_flag": "only_traffic_stats"
  },

  "Network Module":[
    {
      "type": "CheckBox",
      "key": "network-status",
      "title": "Network status",
      "value": true
    },
    {
      "type": "CheckBox",
      "key": "mobile-status",
      "title": "Mobile status"
    }
  ]
}